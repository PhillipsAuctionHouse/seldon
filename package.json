{
  "name": "@phillips/seldon",
  "version": "1.19.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/PhillipsAuctionHouse/seldon"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/": {
      "import": "./dist/"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "start": "storybook dev -p 6006",
    "test": "jest",
    "build": "npm run clean && tsc && vite build",
    "build:storybook": "storybook build",
    "preview": "npx http-server storybook-static",
    "clean": "rimraf './dist'",
    "clean:stories": "rimraf './storybook-static'",
    "format": "prettier . --write",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && npm run lint:styles",
    "lint:styles": "stylelint \"src/**/*.scss\" --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "flatpickr": "^4.6.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@figma/code-connect": "^0.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@storybook/addon-essentials": "^7.6.18",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.6.18",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "@storybook/react-vite": "^7.6.19",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/color": "^3.0.6",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "color": "^4.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.1",
    "prettier": "3.2.5",
    "react": "^18.3.1",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.77.0",
    "semantic-release": "^23.0.8",
    "storybook": "^7.6.19",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.2.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.2.0"
  }
}
